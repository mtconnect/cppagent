<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema' xmlns='urn:mtconnect.org:MTConnectDevices:1.4' xmlns:mt='urn:mtconnect.org:MTConnectDevices:1.4' targetNamespace='urn:mtconnect.org:MTConnectDevices:1.4' elementFormDefault='qualified' attributeFormDefault='unqualified'>
