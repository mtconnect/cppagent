  <xs:simpleType name='ErrorCodeType'>
    <xs:annotation>
      <xs:documentation>
        The error code
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:string'>
      <xs:enumeration value='UNAUTHORIZED'>
        <xs:annotation>
          <xs:documentation>
            The request did not have sufficient permissions to perform the
            request.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='NO_DEVICE'>
        <xs:annotation>
          <xs:documentation>
            The device specified in the URI could not be found.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='OUT_OF_RANGE'>
        <xs:annotation>
          <xs:documentation>
            The sequence number was beyond the end of the buffer.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='TOO_MANY'>
        <xs:annotation>
          <xs:documentation>
            The count given is too large.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='INVALID_URI'>
        <xs:annotation>
          <xs:documentation>
            The URI provided was incorrect.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='INVALID_REQUEST'>
        <xs:annotation>
          <xs:documentation>
            The request was not one of the three specified requests.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='INTERNAL_ERROR'>
        <xs:annotation>
          <xs:documentation>
            Contact the software provider, the agent did not behave correctly.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='INVALID_PATH'>
        <xs:annotation>
          <xs:documentation>
            The xpath could not be parsed. Invalid syntax.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='UNSUPPORTED'>
        <xs:annotation>
          <xs:documentation>
            The request is not supported by this implementation
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='ASSET_NOT_FOUND'>
        <xs:annotation>
          <xs:documentation>
            The asset ID cannot be found
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name='ErrorType'>
    <xs:annotation>
      <xs:documentation>
