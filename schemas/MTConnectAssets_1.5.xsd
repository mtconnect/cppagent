  <xs:complexType name='HeaderType'>
    <xs:annotation>
      <xs:documentation>
        An XML container in an mtconnect response document that provides
        information from an agent defining version information, storage
        capacity, and parameters associated with the data management within the
        agent.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base='xs:string'>
        <xs:attribute name='version' type='VersionType' use='required'>
          <xs:annotation>
            <xs:documentation>
              The protocol version number.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='creationTime' type='CreationTimeType' use='required'>
          <xs:annotation>
            <xs:documentation>
              The time the response was created.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='testIndicator' type='TestIndicatorType' use='optional'>
          <xs:annotation>
            <xs:documentation>
              Optional flag that indicates the system is operating in test mode.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='instanceId' type='InstanceIdType' use='required'>
          <xs:annotation>
            <xs:documentation>
              A number indicating which invocation of the agent.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='sender' type='SenderType' use='required'>
          <xs:annotation>
            <xs:documentation>
              The agent identification information.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='assetBufferSize' type='AssetBufferSizeType' use='required'>
          <xs:annotation>
            <xs:documentation>
              The maximum number of mtconnect asset that will be retained by the
              agent.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='assetCount' type='AssetCountAttrType' use='required'>
          <xs:annotation>
            <xs:documentation>
              The total number of mtconnect asset in anagent.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name='MTConnectAssetsType'>
    <xs:annotation>
      <xs:documentation>
        It is the root XML element of an mtconnectassets response document.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name='Header' type='HeaderType'>
        <xs:annotation>
          <xs:documentation>
            An XML container in an mtconnect response document that provides
            information from an agent defining version information, storage
            capacity, and parameters associated with the data management within
            the agent.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name='Assets' type='AssetsType'>
        <xs:annotation>
          <xs:documentation>
            Used in the path portion of an http request line, by a client
            software application, to initiate an asset request to an agent to
            publish an mtconnectassets document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name='SourceType'>
    <xs:annotation>
      <xs:documentation>
        A URI reference
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:anyURI'/>
  </xs:simpleType>
  <xs:complexType name='AssetDescriptionType' mixed='true'>
    <xs:annotation>
      <xs:documentation>
        The description of an asset, can be free form text or elements
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:any processContents='lax' minOccurs='0' maxOccurs='unbounded'>
        <xs:annotation>
          <xs:documentation>
            A placeholder for an XML element.
          </xs:documentation>
        </xs:annotation>
      </xs:any>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name='AssetsType'>
    <xs:annotation>
      <xs:documentation>
        Used in the path portion of an http request line, by a client software
        application, to initiate an asset request to an agent to publish an
        mtconnectassets document.
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs='0' maxOccurs='unbounded'>
      <xs:element ref='Asset'/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name='AssetType' abstract='true'>
    <xs:annotation>
      <xs:documentation>
        An abstract mobile asset
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name='assetId' type='AssetIdType' use='required'>
      <xs:annotation>
        <xs:documentation>
          The unique identifier for the mtconnect asset. The identifier MUST be
          unique with respect to all other asset in an MTConnect installation.
          The identifier SHOULD be globally unique with respect to all other
          asset.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='timestamp' type='TimestampType' use='required'>
      <xs:annotation>
        <xs:documentation>
          The most accurate time available to a piece of equipment that
          represents the point in time that the data was reported.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='deviceUuid' type='UuidType' use='required'>
      <xs:annotation>
        <xs:documentation>
          The piece of equipments UUID that supplied this data. This is an
          optional element references to the UUID attribute given in the device
          element. This can be any series of numbers and letters as defined by
          the XML type nmtoken.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='removed' type='RemovedType' use='optional'>
      <xs:annotation>
        <xs:documentation>
